{
  "name": "ü§ù Chuwue Grill - Partner B2B Sync",
  "description": "Sincronizaci√≥n bidireccional con FindYourWork\nManeja: √ìrdenes de servicio, disponibilidad, sincronizaci√≥n de eventos\nUsaCRM workflow para eventos corporativos",
  "nodes": [
    {
      "id": "node-1",
      "type": "webhook",
      "position": [100, 100],
      "parameters": {
        "path": "webhooks/partner/service-request",
        "method": "POST"
      },
      "name": "Service Request from FindYourWork"
    },
    {
      "id": "node-2",
      "type": "webhook",
      "position": [100, 250],
      "parameters": {
        "path": "webhooks/partner/availability-check",
        "method": "POST"
      },
      "name": "Availability Check"
    },
    {
      "id": "node-3",
      "type": "http",
      "position": [300, 100],
      "parameters": {
        "url": "http://localhost:8002/payments/process",
        "method": "POST",
        "body": "{\n  \"service_type\": \"={{$json.service_type}}\",\n  \"partner_id\": \"={{$json.partner_id}}\",\n  \"amount\": \"={{$json.amount}}\",\n  \"currency\": \"COP\"\n}"
      },
      "name": "Process Service Payment"
    },
    {
      "id": "node-4",
      "type": "http",
      "position": [300, 250],
      "parameters": {
        "url": "http://localhost:8002/partners/availability",
        "method": "POST",
        "body": "{\n  \"date_range\": \"={{$json.date_range}}\",\n  \"event_id\": \"={{$json.event_id}}\"\n}"
      },
      "name": "Check Restaurant Availability"
    },
    {
      "id": "node-5",
      "type": "http",
      "position": [500, 175],
      "parameters": {
        "url": "http://localhost:8000/webhooks/chuwue/service-status/",
        "method": "POST",
        "body": "{\n  \"event_type\": \"service.status_updated\",\n  \"timestamp\": \"={{$now.toISO()}}\",\n  \"data\": \"={{$json}}\"\n}"
      },
      "name": "Send Service Status to FindYourWork"
    },
    {
      "id": "node-6",
      "type": "no-op",
      "position": [700, 175],
      "name": "Sync Complete"
    }
  ],
  "connections": {
    "node-1": {
      "main": [[{"node": "node-3", "type": "main", "index": 0}]]
    },
    "node-2": {
      "main": [[{"node": "node-4", "type": "main", "index": 0}]]
    },
    "node-3": {
      "main": [[{"node": "node-5", "type": "main", "index": 0}]]
    },
    "node-4": {
      "main": [[{"node": "node-5", "type": "main", "index": 0}]]
    },
    "node-5": {
      "main": [[{"node": "node-6", "type": "main", "index": 0}]]
    }
  }
}