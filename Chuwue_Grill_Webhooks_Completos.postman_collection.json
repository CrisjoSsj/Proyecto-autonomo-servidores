    {
      "name": "üí≥ WEBHOOKS STRIPE",
      "item": [
        {
          "name": "Charge Succeeded",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "pm.environment.set('stripe_timestamp', Math.floor(Date.now() / 1000));",
                  "pm.environment.set('stripe_id', 'evt_test_' + Math.random().toString(36).substr(2, 9));"
                ]
              }
            },
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('‚úÖ Status 200/201 (Exitoso)', function () {",
                  "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
                  "});",
                  "pm.test('‚úÖ Contiene transaction_id', function () {",
                  "    pm.expect(pm.response.json()).to.have.property('status');",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "X-Stripe-Signature",
                "value": "t={{stripe_timestamp}},v1=test_signature"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"id\": \"{{stripe_id}}\",\n  \"type\": \"charge.succeeded\",\n  \"created\": {{stripe_timestamp}},\n  \"data\": {\n    \"object\": {\n      \"id\": \"ch_{{stripe_id}}\",\n      \"object\": \"charge\",\n      \"amount\": 9999,\n      \"currency\": \"usd\",\n      \"customer\": \"cus_12345678\",\n      \"description\": \"Pago por evento corporativo - Chuwue Grill\",\n      \"status\": \"succeeded\",\n      \"paid\": true,\n      \"receipt_email\": \"cliente@chuwuegrill.com\",\n      \"metadata\": {\n        \"order_id\": \"ORD-{{$randomInt(10000, 99999)}}\",\n        \"event_type\": \"corporativo\"\n      }\n    }\n  }\n}"
            },
            "url": {
              "raw": "http://localhost:8002/webhooks/payments/stripe/",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8002",
              "path": ["webhooks", "payments", "stripe", ""]
            },
            "description": "Evento: Pago exitoso en Stripe"
          }
        },
        {
          "name": "Charge Refunded",
          "event": [
            {
              "listen": "test",
              "script": {
